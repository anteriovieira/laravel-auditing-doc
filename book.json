{
  "title": "laravel-auditing",
  "gitbook": ">3.0.0",
  "plugins": ["edit-link", "theme-vuejs", "-fontsettings", "github", "regexplace", "laravel-auditing"],
  "structure": {
    "summary": "documentation.md"
  },
  "pluginsConfig": {
    "edit-link": {
      "base": "https://github.com/owen-it/laravel-auditing-doc/master",
      "label": "Edit This Page"
    },
    "github": {
      "url": "https://github.com/owen-it/laravel-auditing"
    },
    "regexplace": {
      "substitutes": [
        {"pattern": "doc", "flags": "g", "substitute": "__doc"}
      ]
    }
  },
  "variables": {
    "version": "../../"
  }
}